.div.user-table
  %h1 Listing projects

  %table
    %tr
      %th Project Name
      %th{ colspan: 2 }
        Actions
    - @projects.each do |project|
      %tr
        %td= project.name
        %td= link_to 'Edit', edit_project_path(project), data: { turbolinks: false }
        %td= link_to 'Destroy', project, :method => :delete, :data => { :confirm => 'Are you sure?' }
  %br
  = link_to 'New Project', new_project_path
